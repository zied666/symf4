                    </div>
                    <div class="modal-body">
                        <table class="table text-nowrap">
                            <thead>
                            <tr>
                                <th>Produits</th>
                                <th>Temperature</th>
                                <th>Density</th>
                            </tr>
                            </thead>
                            <tbody class="tank-btn">
                            <tr ng-repeat="itemDensityDefault in bargesDefault">
                                <td>
                                    <h6 class="text-semibold no-margin">{[{ itemDensityDefault.product.name }]}</h6>
                                </td>
                                <td>
                                    <h6 class="text-semibold no-margin">
                                        {[{itemDensityDefault.product.temp == null ? '' :
                                        itemDensityDefault.product.temp}]} °C
                                    </h6>
                                </td>
                                <td>
                                    <h6 class="text-semibold no-margin">
                                        {[{itemDensityDefault.product.specificGravity == null ? '' :
                                        itemDensityDefault.product.specificGravity}]}
                                    </h6>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="myModaldensityCustom" role="dialog">
