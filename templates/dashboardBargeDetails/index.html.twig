                <div class="box small" dw-loading="sample-density"
                     dw-loading-options="{active: true, text: '', className: 'custom-loading', spinnerOptions: {lines: 8, length: 10, width: 3, radius: 5, color: '#00844e', direction: -1, speed: 2}}">
                </div>
            </div>
