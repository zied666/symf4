{% extends 'base.html.twig' %}

{% block body %}
    <div style="position: relative">
        <div class="sample sample-2">
            <div class="box small" dw-loading="sample-2"
            dw-loading-options="{active: true, text: '', className: 'custom-loading', spinnerOptions: {lines: 12, length: 20, width: 6, radius: 15, color: '#00844e', direction: -1, speed: 3}}">
            dw-loading-options="{active: true, text: 'Loading', className: 'custom-loading', spinnerOptions: {lines: 12, length: 20, width: 6, radius: 20, color: '#f0f', direction: -1, speed: 3}}">
            </div>
        </div>
        <div class="content content-dash-details">

            <div ui-view>
            </div>

            <div class="barge_list" style="height: 100%" ng-show="showDiv">
                <div class="logos">
                    <div class="logo">
                        <a href="{{ path('dashboard') }}">
                            <img src="{{ asset('custom_assets/images/logo/logo.png') }}" width="80" class="" alt="">
                        </a>
                        <p class="logo_text">POC Vegoil Tracking V.0</p>
                    </div>
                    <button class="btn settings" ng-click="hideConfig = !hideConfig; gotoDash()">
                        <img src="{{ asset('custom_assets/images/icons/settings.png') }}"
                             class=""
                             alt="">
                    </button>
                </div>

                <div class="list-barge-progress first-side" ng-hide="hideConfig">
                    <div class="panel with-nav-tabs panel-default">
                        <div class="panel-heading">
                            <ul class="nav nav-tabs">
                                <li ng-class="{'active': $storage.idType == 'progress'}">
                                    <a href="#tab1default" data-toggle="tab">In progress</a>
                                </li>
                                <li ng-class="{'active': $storage.idType == 'planned'}">
                                    <a class="second-tab" href="#tab2default" data-toggle="tab">Planned</a>
                                </li>
                                <li ng-class="{'active': $storage.idType == 'historic'}">

                                    <a class="third-tab" href="#tab3default" data-toggle="tab">Historic</a>
                                </li>
                            </ul>
                        </div>
                        <div class="panel-body">
                            <div class="tab-content">
                                <div class="tab-pane fade in" ng-class="{'active': $storage.idType == 'progress'}"
