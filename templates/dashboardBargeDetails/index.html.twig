                <form ng-submit="editDensity(myFormDensity)"
                      name="myFormDensity" novalidate>
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close"
                                    data-dismiss="modal">&times;</button>
                            <h4 class="modal-title">Custom density </h4>
                        </div>
                        <div class="modal-body">
                            <div class="alert alert-danger" ng-show="errorMesageShow">
                                <strong>Danger!</strong> {[{ errorMesage }]}
                            </div>
                            <table class="table text-nowrap">
                                <thead>
                                <tr>
                                    <th>Produits</th>
                                    <th>Temperature</th>
                                    <th>Density</th>
                                </tr>
                                </thead>
                                <tbody class="tank-btn">
                                <tr class="inputBlock"
                                    ng-repeat="itemDensityDefault in bargesDefault">
                                    <td>
                                        <h6 class="text-semibold no-margin">
                                            {[{itemDensityDefault.product.name }]}</h6>
                                    </td>
                                    <td>
                                        <input type="text"
                                               ng-model="customdensityTab[$index][2]"
                                               ng-required="true">
                                    </td>
                                    <td>
                                        <input type="text"
                                               ng-model="customdensityTab[$index][1]"
                                               ng-required="true">
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="modal-footer">
                            <button class="btn validate">validate</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>

    </div>
