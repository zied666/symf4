{% extends ":portManagement/port_type:index.html.twig" %}
{% block title %}Port Type{% endblock %}
{% block breadcrumb %}
    {{ parent() }}
    <li><a href="#">Form</a></li>
{% endblock %}
 {% block body %}
     <div class="row">
         <div class="col-sm-offset-2 col-lg-offset-3 col-md-offset-3 col-md-6  col-lg-6 col-sm-8 col-xs-12">
             {{ form_start(form) }}
             <div class="panel panel-flat">
                 <div class="panel-body">
                     <div class="row ">
                         {{ forms.input("col-md-12","Name",form.name) }}
                     </div>
                     {{ buttons.submitForm() }}
                 </div>
             </div>
             {{ form_end(form) }}
         </div>
     </div>
 {% endblock %}

