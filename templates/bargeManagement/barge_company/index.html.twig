{% extends "dashboard/index.html.twig" %}
{% block title %}Barge Companies List{% endblock %}
{% block breadcrumb %}
    <li><a href="{{ path('barge_company_list') }}">Barge Companies List</a></li>
{% endblock %}
 {% block body %}
<div class="content">
     <div class="panel panel-flat">
         <div class="panel-heading-custom">
             {{ buttons.addItem("New Compnay",path("barge_company_form")) }}
         </div>
         <table class="table" id="myTable">
             <thead>
             <tr>
                 <th>Name</th>
                 <th>Actions</th>
             </tr>
             </thead>
         </table>
     </div>
    </div>
 {% endblock %}
{% block javascripts %}
    {{ scripts.dataTable("barge_company_list",'{data: "name"},{data: "actions", orderable: false,width:"10%"}') }}
{% endblock %}
